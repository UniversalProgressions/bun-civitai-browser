{
  "compilerOptions": {
    "paths": {
      "#generated/*": ["./generated/*"],
      "#client/*": ["./src/html/*"],
      "#modules/*": ["./src/modules/*"]
    },
    "target": "esnext",
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": [
      "bun-types"
    ], 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
}
