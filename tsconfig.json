{
  "compilerOptions": {
    "paths": {
      "#generated/*": ["./src/modules/db/generated/*"],
      "#client/*": ["./src/html/*"],
      "#modules/*": ["./src/modules/*"],
      "#civitai-api/*": ["./src/civitai-api/*"]
    },
    "target": "esnext",
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": ["bun-types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
